{{listJson.name}}
<div>
  <table>
    <tr ng-repeat="task in taskJson">
      <td>
        <input type="checkbox" ng-checked="{{task.completed}}">
      </td>
      <td>
        <p>{{task.text}}</p>
      </td>
      <td>
        Delete: <a style="color:red; cursor:pointer" ng-click="echo(listJson, task)">X</a>
      </td>
    </tr>
    <tr>
      <td>
        <input type="checkbox">
      </td>
      <td>
        <input value="">
      </td>
    </tr>
  </table>
</div>